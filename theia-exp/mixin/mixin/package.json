{
  "name": "mixin",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && cat dist/mixin/main.js dist/mixin/polyfills.js dist/mixin/runtime.js > dist/emit.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --config .prettierrc \"src/**/*.{ts,tsx,css,less,scss,sass,js,html}\" --write",
    "format:check": "npm run check:prettier",
    "check:prettier": "prettier --config .prettierrc \"src/**/*.{ts,tsx,css,less,scss,sass,js,html}\" --check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/elements": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "resolutions": {
    "**/@types/node": "16"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-select": "^5.0.1",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "~2.7.1",
    "typescript": "~4.7.2"
  }
}
